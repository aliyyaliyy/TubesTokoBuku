	<queryString language="SQL">
		<![CDATA[SELECT 
    t.idTransaksi,
    t.tanggalTransaksi,
    p.namaPegawai AS namaKasir,
    b.judulBuku AS namaBarang,
    d.jumlah,
    d.hargaSatuan,
    d.subtotal
FROM 
    transaksi t
INNER JOIN 
    datapegawai p ON t.idPegawai = p.idPegawai
INNER JOIN 
    detailTransaksi d ON t.idTransaksi = d.idTransaksi
INNER JOIN 
    dataBuku b ON d.noISBN = b.noISBN]]>
